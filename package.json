{
	"name": "@ajwdmedia/svelterial-icons",
	"version": "0.1.3",
	"scripts": {
		"dev": "vite dev",
		"build:components": "node build.js",
		"build": "node build.js && svelte-kit sync && svelte-package && node finish.js",
		"package": "svelte-package",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"package:last": "node finish.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"@material-design-icons/svg": "^0.12.1"
	},
	"type": "module",
	"dependencies": {}
}
